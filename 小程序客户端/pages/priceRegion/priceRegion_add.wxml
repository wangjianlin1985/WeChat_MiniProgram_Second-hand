<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>
  <view class="title">添加价格区间</view>

  <view class="priceRegion_add_view">
    <form bindsubmit="formSubmit">
      <view class="formItem">
        <view class="label">价格区间：</view>
        <view class="formControl">
          <input type="text" name="priceRegion.regionName" placeholder="请输入价格区间" value="{{regionName}}" />
        </view>
      </view>

      <view>
        <button class="vBtn_ok" formType="submit" disabled="{{disabled}}">提交发布</button>
      </view>

    </form>
  </view>
</view>
